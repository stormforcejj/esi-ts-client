{
    "name": "esi-ts-client",
    "version": "1.0.0",
    "description": "A typescript client for the EvE Online ESI API with caching and rate limiting. Intended for backend use.",
    "main": "index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "generate:api": "openapi-generator-cli generate -i ./esi.json -g typescript-fetch -o ./src/generated -t ./templates",

        "@openapitools/openapi-generator-cli": "^2.21.4",
        "@types/node": "^24.2.1",
        "@types/node-fetch": "^2.6.13",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "eslint": "^9.33.0",
        "node-fetch": "^2.7.0",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
